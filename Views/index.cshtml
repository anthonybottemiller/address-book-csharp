<h1>Address Book<h1>
@if (@Model.Count == 0)
{
 <h2>No contacts exits yet!</h2>
}
@foreach (var contact in Model)
{
 <h3><a href="/contact/@contact.GetId()">@contact.GetName()</a></h3>
}
<a href="/contact/new">New Contact</a>
<form action="/contact/clear" method="post">
  <button type="submit">Clear Contacts</button>
</form>
